// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model old_person {
  dni Int @id
  name String
  lastname String
  password String
}

model f_member {
  dni Int @id
  name String
  lastname String
  password String
}

model nurse {
  dni Int @id
  name String
  lastname String
  password String
}
